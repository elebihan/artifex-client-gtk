<?xml version="1.0" encoding="utf-8"?>
<schemalist>
  <schema path="/com/elebihan/artifex-client-gtk/" id="@app-id@" gettext-domain="@gettext-package@">
    <key name="window-width" type="i">
      <default>800</default>
      <summary>Window width</summary>
    </key>
    <key name="window-height" type="i">
      <default>600</default>
      <summary>Window height</summary>
    </key>
    <key name="is-maximized" type="b">
      <default>false</default>
      <summary>Window maximized state</summary>
    </key>
    <key name="recent-urls" type="as">
        <default>[]</default>
      <summary>Recent URLs</summary>
      <description>This is the list of URLs to servers recently used</description>
    </key>
  </schema>
</schemalist>
